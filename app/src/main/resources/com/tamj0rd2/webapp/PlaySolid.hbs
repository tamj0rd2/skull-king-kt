<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>Playing Skull King</title>
    <link rel="icon" type="image/svg+xml" href="/frontend-solid/vite.svg" />
    <link rel="stylesheet" href="/frontend-solid/index.css"/>
</head>
<style>
    body {
        font-family: "Droid Sans", sans-serif;
    }
</style>
<body>
<div id="root"></div>
<script>
    let wsProtocol = "ws";
    if (location.protocol === "https:") wsProtocol += "s"

    window.INITIAL_STATE = {
        endpoint: `${wsProtocol}://{{host}}:${location.port}/play`,
        ackTimeoutMs: {{ackTimeoutMs}}
    };
</script>
<!--<script defer type="module" src="./entry.js"></script>-->
{{#if devServer}}
    <script defer type="module" src="http://localhost:5173/frontend-solid/@vite/client"></script>
    <script defer type="module" src="http://localhost:5173/frontend-solid/src/index.tsx"></script>
{{else}}
    <script defer type="module" src="/frontend-solid/index.js"></script>
{{/if}}
</body>
</html>
